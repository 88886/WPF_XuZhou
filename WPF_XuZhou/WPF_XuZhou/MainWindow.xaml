<Window x:Class="WPF_XuZhou.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_XuZhou"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        WindowStyle="None"
        AllowsTransparency="True"
        WindowStartupLocation="CenterOwner"
        Background="Transparent"
        Title="徐州专业技术人员继续教育" Height="450" Width="800">
    <Border CornerRadius="10" Background="Transparent"  hc:WindowAttach.IsDragElement="True">
        <Grid >
            <Frame  x:Name="framemain"  BorderThickness="0" NavigationUIVisibility="Hidden"  />
            <ScrollViewer VerticalScrollBarVisibility="Hidden" HorizontalAlignment="Right" >
                <StackPanel  hc:Growl.GrowlParent="True" VerticalAlignment="Top" Margin="0,5,5,5"/>
            </ScrollViewer>
        </Grid>
    </Border>
</Window>
